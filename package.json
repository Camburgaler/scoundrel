{
    "name": "scoundrel",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "upgrade": "rm -rf node_modules package-lock.json && npm install",
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "prepare": "svelte-kit sync || echo ''",
