        "@sveltejs/adapter-vercel": "^0",
        "@sveltejs/kit": "^2.49.2",
        "@sveltejs/vite-plugin-svelte": "^6",
        "@tsconfig/svelte": "^5",
        "@types/express": "^5",
        "@types/node": "^24",
        "@types/pg": "^8",
        "dotenv": "^17",
        "express": "^5",
        "globals": "^16",
        "pg": "^8",
        "svelte": "^5",
        "svelte-check": "^4",
        "svelte-preprocess": "^6",
